<Keyboard
    xmlns:android="http://schemas.android.com/apk/res-auto"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="@dimen/key_bottom_gap"
    android:keyHeight="@dimen/key_height"
>
    <Row
        android:rowEdgeFlags="top"
    >
        <Key
            android:keyLabel="၁"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="1¹½⅓¼⅛"
            android:keyEdgeFlags="left" />
        <Key
            android:keyLabel="၂"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="2²⅔" />
        <Key
            android:keyLabel="၃"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="3³¾⅜" />
        <Key
            android:keyLabel="၄"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="4" />
        <Key
            android:keyLabel="၅"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="5⅝" />
        <Key
            android:keyLabel="၆" 
			android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="6" />
        <Key
            android:keyLabel="၇"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="7⅞" />
        <Key
            android:keyLabel="၈" 
			android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="8" />
        <Key
            android:keyLabel="၉" 
			android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="9" />
        <Key
            android:keyLabel="၀"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters="0"
            android:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key
            android:keyLabel="ဧ"
            android:keyEdgeFlags="left" />
        <Key
            android:keyLabel="၏" />
        <Key
            android:keyLabel="ဣ" />
        <Key
            android:keyLabel="ဤ" />
        <Key
			android:keyLabel="၌" />
        <Key
            android:keyLabel="၍" />
        <Key
            android:keyLabel="ဈ" />
        <Key
            android:keyLabel="ဎ" />
        <Key
            android:keyLabel="၎" />
        <Key
            android:keyLabel="႑"
            android:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key
            android:codes="@integer/key_shift"
            android:keyLabel="@string/label_alt_key"
			android:hidePopupPreview="true"
            android:keyWidth="15%p"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left" />
        <Key
            android:keyLabel="ဦ" />
        <Key
            android:keyLabel="ၮ" />
        <Key
            android:keyLabel="ၯ" />
        <Key
            android:keyLabel="႗" />
        <Key
            android:keyLabel="႒" />
        <Key
            android:keyLabel="ဋ" />
        <Key
            android:keyLabel="ဍ" />
        <Key
            android:codes="@integer/key_delete"
            android:keyIconId="@integer/icon_keyboard_delete"
			android:hidePopupPreview="true"
            android:keyWidth="15%p"
            android:isModifier="true"
            android:isRepeatable="true"
            android:keyEdgeFlags="right" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_symbols"
        android:rowEdgeFlags="bottom"
    >
        <Key
            android:codes="@integer/key_symbol"
            android:keyLabel="@string/label_alpha_key"
			android:hidePopupPreview="true"
            android:keyWidth="15%p"
            android:isModifier="true"
            android:keyEdgeFlags="left" />
        <Key
            android:keyLabel="&#x1088;"
			android:popupKeyboard="@xml/kbd_popup_wide_template"
            android:popupCharacters="@string/alternates_for_cyrillic_e" />
        <Key
            android:codes="@integer/key_space"
            android:keyIconId="@integer/icon_keyboard_space"
            android:keyWidth="40%p"
            android:isModifier="true" />
        <Key
            android:codes="@integer/key_f1"
            android:isModifier="true" />
        <Key
            android:keyLabel="."
            android:keyIconId="@integer/icon_keyboard_hint_popup"
            android:popupKeyboard="@xml/popup_punctuation"
            android:isModifier="true" />
        <Key
            android:codes="@integer/key_return"
            android:keyIconId="@integer/icon_keyboard_return"
			android:hidePopupPreview="true"
            android:keyWidth="15%p"
            android:isModifier="true"
            android:keyEdgeFlags="right" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_symbols_with_settings_key"
        android:rowEdgeFlags="bottom"
    >
        <Key
            android:codes="@integer/key_symbol"
            android:keyLabel="@string/label_alpha_key"
			android:hidePopupPreview="true"
            android:keyWidth="15%p"
            android:isModifier="true"
            android:keyEdgeFlags="left" />
        <Key
            android:codes="@integer/key_settings"
            android:keyIconId="@integer/icon_keyboard_settings"
			android:hidePopupPreview="true"
            android:isModifier="true" />
        <Key
            android:keyLabel="&#x1088;"
			android:popupKeyboard="@xml/kbd_popup_wide_template"
            android:popupCharacters="@string/alternates_for_cyrillic_e" />
        <Key
            android:codes="@integer/key_space"
            android:keyIconId="@integer/icon_keyboard_space"
            android:keyWidth="30%p"
            android:isModifier="true" />
        <Key
            android:codes="@integer/key_f1"
            android:isModifier="true" />
        <Key
            android:keyLabel="."
            android:keyIconId="@integer/icon_keyboard_hint_popup"
            android:popupKeyboard="@xml/popup_punctuation"
            android:isModifier="true" />
        <Key
            android:codes="@integer/key_return"
            android:keyIconId="@integer/icon_keyboard_return"
			android:hidePopupPreview="true"
            android:keyWidth="15%p"
            android:isModifier="true"
            android:keyEdgeFlags="right" />
    </Row>
</Keyboard>
